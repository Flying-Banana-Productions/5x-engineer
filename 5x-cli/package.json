{
  "name": "5x-cli",
  "version": "0.1.0",
  "type": "module",
  "bin": {
    "5x": "./src/bin.ts"
  },
  "exports": {
    ".": "./src/index.ts"
  },
  "scripts": {
    "dev": "bun run src/bin.ts",
    "test": "bun test --concurrent --dots",
    "build": "bun build --compile src/bin.ts --outfile dist/5x",
    "lint": "bunx --bun @biomejs/biome check src/ test/",
    "typecheck": "bunx --bun tsc --noEmit"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.7.0"
  },
  "dependencies": {
    "citty": "^0.1.6",
    "yaml": "^2.7.0",
    "zod": "^3.24.0"
  }
}
